{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@components": ["src/components"],
      "@pages": ["src/pages"],
      "@enums": ["src/enums"],
      "@routes": ["src/routes/*"],
      "@utils": ["src/utils/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/services/*"],
      "@contexts/*": ["src/contexts/*"],
      "@styles/*": ["src/styles/*"],
    },
  }
}
